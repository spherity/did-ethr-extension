{
  "IEthrDidExtension": {
    "components": {
      "schemas": {
        "IEthrChangeControllerKeyArgs": {
          "type": "object",
          "properties": {
            "did": {
              "type": "string",
              "description": "The identity's DID to change the controller key for"
            },
            "kid": {
              "type": "string",
              "description": "kid of the new controller key"
            }
          },
          "required": [
            "did",
            "kid"
          ],
          "description": "Arguments needed for {@link EthrDidExtension.ethrChangeControllerKey}"
        }
      },
      "methods": {
        "ethrChangeControllerKey": {
          "description": "Change controller/ owner of an did:ethr identity",
          "arguments": {
            "$ref": "#/components/schemas/IEthrChangeControllerKeyArgs"
          },
          "returnType": {
            "type": "string",
            "description": "The transaction hash of the changeOwner transaction"
          }
        }
      }
    }
  }
}